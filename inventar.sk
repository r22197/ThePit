on first join:
	set {inventoryslot::%player%::sword} to 0

command /inventar:
	aliases: /inv, /inventory
	trigger:
		close player's inventory
		wait 2 ticks	
		open chest with 6 rows named "&eUpravujes si inventar.." to player
		wait 1 tick

		set {_blackSlots::*} to 9, 10, 11, 12, 13, 14, 15, 16, 17, 45, 46, 47, 48, 49, 50, 51, 52 and 53
		format a gui slot {_blackSlots::*} of player with Black Stained Glass Pane named "&7:^)" 

		format a gui slot 0 of player with rose red named "&cOpustit menu" with lore "||&eKlikni zde pro hlavni menu!||&r" to run:
			make player execute "/menu"
		format a gui slot 4 of player with clock named "&cResetovat inventar" with lore "||&eKlikni zde pro resetovani inventare!||&r" to run:
			close player's inventory
			set {inventoryslot::%player%::sword} to 0
			if {inventoryslot::%player%::bow} is set:
				set {inventoryslot::%player%::bow} to 1
			if {inventoryslot::%player%::fishingrod} is set:
				set {inventoryslot::%player%::fishingrod} to 2
			if {inventoryslot::%player%::lava} is set:
				set {inventoryslot::%player%::lava} to 3
			if {inventoryslot::%player%::pickaxe} is set:
				set {inventoryslot::%player%::pickaxe} to 4
			if {inventoryslot::%player%::arrow} is set:
				set {inventoryslot::%player%::arrow} to 8
			make player execute "inventar"

		set {_slot} to 27
		set {_inventory} to 1
		loop 9 times:
			format a gui slot {_slot} of player with Red Stained Glass named "&eSlot &6##%{_inventory}%" with lore "||&8&l ▶ &7Klikni zde pro nastaveni||    &7predmetu pro tento &dslot&7.||&r" to run:
				close player's inventory
				open chest with 6 rows named "&eUpravujes slot &6##%{_inventory}%&e.." to player
