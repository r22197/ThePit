function PremiumGUI(zl: number, pvpc: number, i: item, output: text, n: text, l: text, p: player):
	set {_x} to 0
	loop 54 times:
		format a gui slot {_x} of {_p} with air
		add 1 to {_x}

	if {_p} has permission "pit.premiumobchod.access":
		set {_v} to "&7Klikni zde pro zaplaceni PvPCoiny."
	else:
		set {_v} to "&cNemas dostatecne opravneni."

	set {_shop} to {gold::%{_p}%}
	replace all "." in {_shop} with ""
	set {_shop} to {_shop} parsed as number

	set {_blackSlots::*} to 9, 10, 11, 12, 13, 14, 15, 16, 17, 45, 46, 47, 48, 49, 50, 51, 52, 53, 22 and 40
	format a gui slot {_blackSlots::*} of {_p} with Black Stained Glass Pane named "&7:^)" 

	create a gui slot 0 of {_p} with rose red named "&cJit zpatky" with lore "||&eKlikni zde pro hlavni menu!||&r" to run:
		make {_p} execute "premiumobchod"
	set {_tokeny::%{_p}%} to placeholder "tm_tokens_raw" from {_p} parsed as number
	set {_token} to splitCoins("%{_tokeny::%{_p}%}%")
	create a gui slot 4 of {_p} with skull of ("%{_p}%" parsed as an offline player) named "&6Postava" with lore "||&5Tva &lekonomika&5!|| &8▪ &7Zlataky: &e%{gold::%{_p}%}% Ⓩ|| &8▪ &7PvPCoiny: &6%{_token}% Ⓟ||&7&o(Lze zakoupit v obchode! &d&o/obchod&7&o)||&r"

	create a gui slot 31 of {_p} with {_i} named "%{_n}%" with lore "%{_l}%"

	set {_x} to splitCoins("%{_zl}%")
	set {_goldslot::*} to 18, 19, 20, 21, 27, 28, 29, 30, 36, 37, 38 and 39
	create a gui slot {_goldslot::*} of {_p} with Yellow Stained Glass Pane named "&e&lZaplatit Zlataky" with lore "||&7Predmet stoji &e%{_x}% Ⓩ&7.||&f ➥ &7Klikni zde pro zaplaceni zlataky." to run:
		if {_p} doesn't have permission "pit.premiumobchod.access":
			stop
		if {_shop} is not higher or equal to {_zl}:
			stop
		close {_p}'s inventory
		play "ORB_PICKUP" to {_p}
		if "%{_n}%" doesn't contain "Booster Zlataku" or "XP Booster" or "Upiri lektvar":
			give {_p} {_i} named "%{_n}%" with lore "%{_l}%"
		else if "%{_n}%" contains "Upiri lektvar":
			give {_p} {_i} with no nbt named "%{_n}%" with lore "%{_l}%"
		else if "%{_n}%" contains "Booster Zlataku":
			add 60 to {%{_p}%.gbooster}
		else if "%{_n}%" contains "XP Booster":
			add 60 to {%{_p}%.xpbooster}
		Zlataky("remove", {_zl}, {_p})
		message ""
		message "&aUspesne jsi zakoupil %{_output}%&a."
		message "&f ➥ &7Z uctu ti bylo odebrano &e%{_x}% Ⓩ&7."
		message ""
		if "%now%" contains "Sun":
			Quest("sunday3", 50, {_p})
		make {_p} execute "premiumobchod"

	set {_y} to splitCoins("%{_pvpc}%")
	set {_pvpcslot::*} to 23, 24, 25, 26, 32, 33, 34, 35, 41, 42, 43  and 44
	create a gui slot {_pvpcslot::*} of {_p} with Orange Stained Glass Pane named "&6&lZaplatit PvPCoiny" with lore "||&7Predmet stoji &6%{_y}% Ⓟ&7.||&f ➥ %{_v}%" to run:
		if {_tokeny::%{_p}%} is not higher or equal to {_pvpc}:
			stop
		close {_p}'s inventory
		play "ORB_PICKUP" to {_p}
		if "%{_n}%" doesn't contain "Booster Zlataku" or "XP Booster":
			give {_p} {_i} named "%{_n}%" with lore "%{_l}%"
		else if "%{_n}%" contains "Booster Zlataku":
			add 60 to {%{_p}%.gbooster}
		else if "%{_n}%" contains "XP Booster":
			add 60 to {%{_p}%.xpbooster}
		message ""
		message "&aUspesne jsi zakoupil %{_output}%&a."
		make console execute command "/tm remove %{_p}% %{_pvpc}%"
		message ""
		if "%now%" contains "Sun":
			Quest("sunday3", 50, {_p})
		make {_p} execute "premiumobchod"

