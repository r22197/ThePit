on damage:
	check ["%region at victim%" doesn't contain "nopvpzone"]->["%region at attacker%" doesn't contain "nopvpzone"]
	victim is a player
	attacker is a player
	if {activatedperk1::%attacker%} or {activatedperk2::%attacker%} or {activatedperk3::%attacker%} or {activatedperk4::%attacker%} is "1ststrike":
		if {1ststrike::%attacker%::%victim%} is not set:
			set {1ststrike::%attacker%::%victim%} to true
			set damage to damage / 100 * (100 + 35)

on death of a player:
	if {activatedperk1::%victim%} or {activatedperk2::%victim%} or {activatedperk3::%victim%} or {activatedperk4::%victim%} is "1ststrike":
		delete {1ststrike::%victim%::*}

on quit:
	if {activatedperk1::%player%} or {activatedperk2::%player%} or {activatedperk3::%player%} or {activatedperk4::%player%} is "1ststrike":
		delete {1ststrike::%player%::*}


on death of a player:
	attacker is a player
	if {renown::%attacker%::tenacity} is 1:
		heal attacker by 0.5 heart
	if {renown::%attacker%::tenacity} is 2:
		heal attacker by 1 heart

on death of player:
	attacker is a player
	if {activatedperk1::%attacker%} or {activatedperk2::%attacker%} or {activatedperk3::%attacker%} or {activatedperk4::%attacker%} is "dirty":
		if attacker has resistance:
			set {_tier} to tier of potion effect resistance of attacker
			if {_tier} is less or equal to 2:
				remove resistance from attacker
				apply potion of resistance of tier 2 to attacker for 5 seconds
		else:
			apply potion of resistance of tier 2 to attacker for 5 seconds
