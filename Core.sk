#==============================[ignore]==============================#			
command /ignore [<offlineplayer>]:
	trigger:
		if arg-1 is not set:
			if size of {bchat::player::%uuid of player%::blocklist::*} is greater than 0:
				message "&dMomentalne ignorujes&7:"
				loop {bchat::player::%uuid of player%::blocklist::*}:
					message "%loop-value%"
			else:
				message "&7Momentalne nikoho neignorujes."
		else:
			if player is online:
				if {ignore.%player%.%arg 1%} is set:
					set {_p} to arg 1
					if {_p} is not set:
						set {_p} to arg 1
					set {_u} to uuid of {_p}	
					delete {bchat::player::%uuid of player%::blocklist::%{_u}%}
					delete {ignore.%player%.%arg 1%}
					send "&7Prestal jsi ignorovat %coloured {_p}'s offline prefix%%{_p}%&7."
				else:
					if arg 1 is player:
						message "&cNemuzes ignorovat sam sebe."
						stop
					if arg 1 has permission "pit.tym":
						message "&cStaff nemuze by ignorovan."
						stop
					if player has permission "pit.tym":
						message "&cJako staff nemuzes ignorovat ostatni."
						stop
					if arg 1 is set:
						set {_p} to arg 1
						if {_p} is not set:
							set {_p} to arg 1
						set {_u} to uuid of {_p}
						set {bchat::player::%uuid of player%::blocklist::%{_u}%} to "&7 * %coloured {_p}'s offline prefix%%{_p}%"
						message "&7Zacal jsi ignorovat %coloured {_p}'s offline prefix%%{_p}%&7."
						set {ignore.%player%.%arg 1%} to true
	
		

	
	
