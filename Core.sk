command /list:
	aliases: /online
	trigger:
		loop all players:
			{vanish::%loop-player%} is not set
			if loop-player's offline prefix contains "Owner":
				set {_1st.list::%loop-player%} to 100
			else if loop-player's offline prefix contains "Admin":
				set {_1st.list::%loop-player%} to 95
			else if loop-player's offline prefix contains "Developer":
				set {_1st.list::%loop-player%} to 85
			else if loop-player's offline prefix contains "Media":
				set {_1st.list::%loop-player%} to 50
			else if loop-player's offline prefix contains "Eventer":
				set {_1st.list::%loop-player%} to 25
			else if loop-player's offline prefix contains "Builder":
				set {_1st.list::%loop-player%} to 20
			else if loop-player's offline prefix contains "Helper":
				set {_1st.list::%loop-player%} to 15
			else if loop-player's offline prefix contains ".Eventer":
				set {_1st.list::%loop-player%} to 14
			else if loop-player's offline prefix contains ".Builder":
				set {_1st.list::%loop-player%} to 12
			else if loop-player's offline prefix contains ".Helper":
				set {_1st.list::%loop-player%} to 10
			else if loop-player's offline prefix contains "Ultimate":
				set {_1st.list::%loop-player%} to 8
			else if loop-player's offline prefix contains "Mythical":
				set {_1st.list::%loop-player%} to 7
			else if loop-player's offline prefix contains "Legendary":
				set {_1st.list::%loop-player%} to 6
			else if loop-player's offline prefix contains "Immortal":
				set {_1st.list::%loop-player%} to 5
			else if loop-player's offline prefix contains "Exalted":
				set {_1st.list::%loop-player%} to 4
			else if loop-player's offline prefix contains "CheatHunter":
				set {_1st.list::%loop-player%} to 3
			else if loop-player's offline prefix contains "Heroic":
				set {_1st.list::%loop-player%} to 3
			else if loop-player's offline prefix contains "Mighty":
				set {_1st.list::%loop-player%} to 2
			else:
				set {_1st.list::%loop-player%} to 1
		set {_list::*} to sorted {_1st.list::*} from highest to lowest with output "@index"
		loop {_list::*}:
			set {_list::%loop-index%} to "%coloured ({_list::%loop-index%} parsed as offlineplayer)'s offline suffix%%({_list::%loop-index%} parsed as offlineplayer)%"
		if {_list::*} is not set:
			set {_list::*} to "&7Nikdo neni online"
		set {_list::*} to "%{_list::*}%"
		replace all " and " in {_list::*} with "&f, "
		replace all ", " in {_list::*} with "&f, "
		message "&eNa serveru se momentalne nachazi &a%number of all players%&7/&f100 &ehracu."
		message "&7(&f%number of all players%/100&7): %{_list::*}%&f."





command /napad [<text>]:
	aliases: /idea, /navrh
	trigger:
		if arg-1 is not set:
			send "&cZapomnel jsi uvest zpravu." to player
			send "&3 * &d/napad &8<&fzprava&8>" to player
			stop trigger
		if {napady} is not set:
			set {napady} to 0
		add 1 to {napady}
		set {napady::%{napady}%} to "&8[&f%{napady}%&8] %coloured player's offline prefix%%player%&7: &e%arg-1%"
		message "&aUspesne jsi napsal napad. &7Napadem bylo &8""&f%arg-1%&8""&7."

command /napady:
	aliases: /ideas
	permission: pit.admin
	permission message: §8[§c§l!§8] §7Tenhle príkaz nebyl bohuzel nalezen, pokud potrebujes poradit, neváhej se obrátit na helpera.	
	trigger:
		if {napady::*} is not set:
			message "&cZatim nikdo nenapsal napad."
			stop
		message "&7&m-------------------------------------------------"
		loop {napady::*}:
			message "%loop-value%"
		message ""
		message "&3 * &d/delnapad &8<&fcislo&8> &8- &7Odstraneni urciteho napadu."
		message "&7&m-------------------------------------------------"

command /delnapad [<integer>]:
	aliases: /deletenapad, /delidea, /deleteidea
	permission: pit.admin
	permission message: §8[§c§l!§8] §7Tenhle príkaz nebyl bohuzel nalezen, pokud potrebujes poradit, neváhej se obrátit na helpera.	
	trigger:
		if arg-1 is not set:
			send "&cZapomnel jsi uvest cislo napadu." to player
			send "&3 * &d/delnapad &8<&fcislo&8>" to player
			stop
		if {napady::%arg 1%} is not set:
			message "&cNapad s timto cislem neexistuje."
			stop
		else if {napady::%arg 1%} is set:
			delete {napady::%arg 1%}
			message "&eNapad &6##%arg 1% &7byl odstranen."
			stop
