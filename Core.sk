			loop {water::*}:
				if location at block is {water::%loop-index%}:
					message "&cBlock byl uspesne odstranen."
					set block at {water::%loop-index%} to air
					delete {water::%loop-index%}
					stop
			message "&cTento block nepatri do vody."
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&cLava Setup":
		cancel event
		if clicked block is set:
			loop {lava::*}:
				if location at block is {lava::%loop-index%}:
					message "&cBlock byl uspesne odstranen."
					set block at {lava::%loop-index%} to air
					delete {lava::%loop-index%}
					stop
			message "&cTento block nepatri do lavy."
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&d&lDort Setup":
		cancel event
		if clicked block is set:
			set {cake.loc::1} to block's location
			message "&aRoh &f##&l1 &auspesne nastaven."
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&6&lRaffle Setup":
		cancel event
		if clicked block is set:
			set {raffle.loc::1} to block's location
			message "&aRoh &f##&l1 &auspesne nastaven."
		else:
			message "&cMusis kliknout na block."


on script load:
	loop {golds::*}:
		set block at {golds::%loop-index%} to gold ore
	loop {water::*}:
		set block at {water::%loop-index%} to water
	loop {lava::*}:
