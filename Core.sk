			stop

on right click with stick:
	player has permission "pit.admin"
	if name of held item is "&6Gold Setup":
		if clicked block is set:
			loop {golds::*}:
				if location at block is {golds::%loop-index%}:
					message "&cTento block je jiz v seznamu goldu."
					stop
			message "&aBlock byl uspesne pridan do goldu."
			add 1 to {golds}
			set {golds::%{golds}%} to location at block
			set block at {golds::%{golds}%} to gold ore
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&bWater Setup":
		if clicked block is set:
			loop {water::*}:
				if location at block is {water::%loop-index%}:
					message "&cTento block je jiz v seznamu vody."
					stop
			message "&aBlock byl uspesne pridan do vody."
			add 1 to {block}
			set {water::%{block}%} to location at block
			set block at {water::%{block}%} to water
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&cLava Setup":
		if clicked block is set:
			loop {lava::*}:
				if location at block is {lava::%loop-index%}:
					message "&cTento block je jiz v seznamu lavy."
					stop
			message "&aBlock byl uspesne pridan do lavy."
			add 1 to {block}
			set {lava::%{block}%} to location at block
			set block at {lava::%{block}%} to lava
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&d&lDort Setup":
		cancel event
		if clicked block is set:
			set {cake.loc::2} to block's location
			message "&aRoh &f##&l2 &auspesne nastaven."
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&6&lRaffle Setup":
		cancel event
		if clicked block is set:
			set {raffle.loc::2} to block's location
			message "&aRoh &f##&l2 &auspesne nastaven."
		else:
			message "&cMusis kliknout na block."


on left click with stick:
	player has permission "pit.admin"
	if name of held item is "&6Gold Setup":
		cancel event
		if clicked block is set:
			loop {golds::*}:
				if location at block is {golds::%loop-index%}:
					message "&cBlock byl uspesne odstranen."
					set block at {golds::%loop-index%} to stone
					delete {golds::%loop-index%}
					stop
			message "&cTento block nepatri do goldu."
		else:
			message "&cMusis kliknout na block."
	if name of held item is "&bWater Setup":
		cancel event
