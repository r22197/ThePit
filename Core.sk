command /drop:
	permission: pit.admin
	permission message: §8[§c§l!§8] §7Tenhle príkaz nebyl bohuzel nalezen, pokud potrebujes poradit, neváhej se obrátit na helpera.	
	trigger:
		if {drops} is not set:
			set {drops} to 0
		add 1 to {drops}
		set {loc::%{drops}%} to player's location
		add 1.5 to y-coordinate of {loc::%{drops}%}
		set {drops::%{drops}%} to "&8[&f%{drops}%&8] %coloured player's offline prefix%%player%&7: &e%{loc::%{drops}%}%"
		message "&7Uspesne jsi nastavil drop lokaci pro &6gold&7."

command /drops:
	permission: pit.admin
	permission message: §8[§c§l!§8] §7Tenhle príkaz nebyl bohuzel nalezen, pokud potrebujes poradit, neváhej se obrátit na helpera.	
	trigger:
		if {drops::*} is not set:
			message "&cZatim neni nastaven zadny drop."
			stop
		message "&7&m-------------------------------------------------"
		loop {drops::*}:
			message "%loop-value%"
		message ""
		message "&3 * &d/deldrop &8<&fcislo&8> &8- &7Odstraneni urciteho dropu."
		message "&7&m-------------------------------------------------"

command /deldrop [<integer>]:
	aliases: /deletedrop
	permission: pit.admin
	permission message: §8[§c§l!§8] §7Tenhle príkaz nebyl bohuzel nalezen, pokud potrebujes poradit, neváhej se obrátit na helpera.	
	trigger:
		if arg-1 is not set:
			send "&cZapomnel jsi uvest cislo dropu." to player
