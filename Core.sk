on rightclick on a ender chest:
	if "%region at player%" contains "nopvpzone":
		if {level::%player%} is less than 10:
			cancel event
			send "&7Musis mit level &a+&910&7, aby jsi mel pristup do &d&lMagické bedny&7."
		else:
			play "CHEST_OPEN" to player
			cancel event
			if check [player has permission "pit.ec.1"]->[player has permission "pit.ec.2"]:
				wait 2 ticks
				open chest with 5 rows named "&eKoukás do magické bedny..&a" to player
				set {_num} to 0			
				loop 45 times:
					set slot {_num} of player's current inventory to {enderchest::%{_num}%::%player%}
					add 1 to {_num}
				stop
			if check [player has permission "pit.ec.1"]->[player doesn't have permission "pit.ec.2"]:
				wait 2 ticks
				open chest with 5 rows named "&eKoukás do magické bedny..&b" to player
				set {_num} to 0			
				loop 36 times:
					set slot {_num} of player's current inventory to {enderchest::%{_num}%::%player%}
					add 1 to {_num}
				wait 1 tick
				set {_num} to 36
				loop 9 times:
					format a gui slot {_num} of player with Orange Stained Glass Pane named "&aBonus radek ##2" with lore "||&8&l⚔ &7Dostupne pouze pro rank &a+&6Legendary&7. &8&l⚔||||&7Zakoupit si ho muzes &ezde&7!||&f ➥ &9&nobchod.pvpcraft.cz||&r" 
					add 1 to {_num}		
				stop		
			if check [player doesn't have permission "pit.ec.1"]->[player doesn't have permission "pit.ec.2"]:
				wait 2 ticks
				open chest with 5 rows named "&eKoukás do magické bedny..&f" to player
				set {_num} to 0	
				loop 27 times:
					set slot {_num} of player's current inventory to {enderchest::%{_num}%::%player%}
					add 1 to {_num}	
				wait 1 tick
				set {_num} to 27
				loop 9 times:
					format a gui slot {_num} of player with Cyan Stained Glass Pane named "&aBonus radek ##1" with lore "||&8&l⚔ &7Dostupne pouze pro rank &a+&bExalted&7. &8&l⚔||||&7Zakoupit si ho muzes &ezde&7!||&f ➥ &9&nobchod.pvpcraft.cz||&r" 
					add 1 to {_num}
				wait 1 tick
				set {_num} to 36	
				loop 9 times:
					format a gui slot {_num} of player with Orange Stained Glass Pane named "&aBonus radek ##2" with lore "||&8&l⚔ &7Dostupne pouze pro rank &a+&6Legendary&7. &8&l⚔||||&7Zakoupit si ho muzes &ezde&7!||&f ➥ &9&nobchod.pvpcraft.cz||&r" 
					add 1 to {_num}	
				stop		
	else:
		if player's gamemode is not creative:
			cancel event	
				
	
on inventory close:				
	if inventory name of player's current inventory contains "&eKoukás do magické bedny..&a":
		play "CHEST_CLOSE" to player
		set {_num} to 0
		loop 45 times:
			if slot {_num} of player's current inventory is not air:
				set {enderchest::%{_num}%::%player%} to slot {_num} of player's current inventory
			else:
				if {enderchest::%{_num}%::%player%} is set:
					delete {enderchest::%{_num}%::%player%}
			add 1 to {_num}
		stop
	else if inventory name of player's current inventory contains "&eKoukás do magické bedny..&b":
		play "CHEST_CLOSE" to player
		set {_num} to 0
		loop 36 times:
			if slot {_num} of player's current inventory is not air:
				set {enderchest::%{_num}%::%player%} to slot {_num} of player's current inventory
			else:
				if {enderchest::%{_num}%::%player%} is set:
					delete {enderchest::%{_num}%::%player%}
			add 1 to {_num}
		stop
	else if inventory name of player's current inventory contains "&eKoukás do magické bedny..&f":
		play "CHEST_CLOSE" to player
		set {_num} to 0
		loop 27 times:
			if slot {_num} of player's current inventory is not air:
				set {enderchest::%{_num}%::%player%} to slot {_num} of player's current inventory
			else:
				if {enderchest::%{_num}%::%player%} is set:
					delete {enderchest::%{_num}%::%player%}
			add 1 to {_num}
		stop	


on chest add:
	if inventory name of player's current inventory contains "&eKoukás do magické bedny.." or "&fTy & &f%{tradePartner::%player%}%":
		if item is stone sword or stone axe or iron sword or iron axe or bow or chain boots or chain leggings or chain chestplate or iron pickaxe or diamond pickaxe or golden apple or lava bucket:
			cancel event
			message "&cTento item nelze ulozit."
			play "VILLAGER_NO" to player
		else if item is player head:
			if name of item is "&6Zlata Hlava":
				cancel event
				message "&cTento item nelze ulozit."
				play "VILLAGER_NO" to player
		else if item is fishing rod:
			if name of item is not "&cCatcher":
				cancel event
				message "&cTento item nelze ulozit."
				play "VILLAGER_NO" to player
		else if item is arrow:
			if name of item is "&7Síp":
				cancel event
				message "&cTento item nelze ulozit."
				play "VILLAGER_NO" to player
