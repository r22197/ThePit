command /bugreport [<text>]:
	aliases: /bug
	trigger:
		if arg-1 is not set:
			send "&cZapomnel jsi uvest zpravu." to player
			send "&3 * &d/bugreport &8<&fzprava&8>" to player
			stop trigger
		if {reports} is not set:
			set {reports} to 0
		add 1 to {reports}
		set {reports::%{reports}%} to "&8[&f%{reports}%&8] %coloured player's offline prefix%%player%&7: &e%arg-1%"
		message "&aUspesne jsi nahlasil chybu. &7Duvodem bylo &8""&f%arg-1%&8""&7."

