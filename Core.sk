options:
	NoPermissions: §8[§c§l!§8] §7Tenhle príkaz nebyl bohuzel nalezen, pokud potrebujes poradit, neváhej se obrátit na helpera.
	
	ChangedGamemodeToSurvival: &7Tvuj gamemode byl zmenen uspesne na &aSurvival&7.
	ChangedGamemodeToCreative: &7Tvuj gamemode byl zmenen uspesne na &dCreative&7.
	ChangedGamemodeToSpectator: &7Tvuj gamemode byl zmenen uspesne na &eSpectator&7.
	ChangedGamemodeToAdventure: &7Tvuj gamemode byl zmenen uspesne na &6Adventure&7.


#==============================[Gamemode]==============================#
command /gamemode [<text>] [<offlineplayer>]:
	aliases: /gm
	permission: pit.admin
	permission message: {@NoPermissions}
	trigger:
		if player has permission "pit.admin":
			if arg-2 is set:
				if arg-2 is offline:
					send "%arg 2% je offline." to player
					stop
				if arg-2 is online:
					if arg 1 is "0" or "survival" or "s":
						set arg-2's gamemode to survival
						send "{@ChangedGamemodeToSurvival}" to arg-2
						send "&7Gamemode hrace %coloured arg-2's offline prefix%%arg-2% &7byl zmenen uspesne na &aSurvival&7." to player
						stop
				if arg 1 is "1" or "c" or "creative":
					set arg-2's gamemode to creative
					send "&7Gamemode hrace %coloured arg-2's offline prefix%%arg-2% &7byl zmenen uspesne na &dCreative&7." to player
					send "{@ChangedGamemodeToCreative}" to arg-2
					stop
				if arg 1 is "3" or "spectator":
					set arg-2's gamemode to spectator
					send "&7Gamemode hrace %coloured arg-2's offline prefix%%arg-2% &7byl zmenen uspesne na &eSpectator&7." to player
