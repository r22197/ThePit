on damage:
	check ["%region at victim%" doesn't contain "nopvpzone"]->["%region at attacker%" doesn't contain "nopvpzone"]
	set damage to damage / 100 * (100 + 1 * {tier.perk::%attacker%::meleedamage})

on damage:
	check ["%region at victim%" doesn't contain "nopvpzone"]->["%region at attacker%" doesn't contain "nopvpzone"]
	set damage to damage / 100 * (100 + 1 * {tier.perk::%attacker%::damagereduction})

on damage:
	check ["%region at victim%" doesn't contain "nopvpzone"]->["%region at attacker%" doesn't contain "nopvpzone"]
	projectile exists
	projectile is arrow
	set damage to damage / 100 * (100 + 3 * {tier.perk::%attacker%::bowdamage})

	if {activatedperk1::%attacker%} or {activatedperk2::%attacker%} or {activatedperk3::%attacker%} or {activatedperk4::%attacker%} is "spammer":
		set {double.spammer::%attacker%::%victim%} to 1


on damage:
	check ["%region at victim%" doesn't contain "nopvpzone"]->["%region at attacker%" doesn't contain "nopvpzone"]
	{activatedperk1::%attacker%} or {activatedperk2::%attacker%} or {activatedperk3::%attacker%} or {activatedperk4::%attacker%} is "bountyhunter"
	{strknumb::%victim%} is set
	set {_x} to damage
	set {_z} to {strknumb::%victim%}
	replace all "." in {_z} with ""
	set {_z} to {_z} parsed as number
	set damage to damage / 100 * (100 + ({_z} / 50))
	send actionbar "&cBounty Reaper&7: &c%{_x}%4❤ &7➠ &c%damage%4❤" to attacker
on quit:
	delete {double.spammer::%player%::*}

on skript start:
	delete {double.spammer::*}

